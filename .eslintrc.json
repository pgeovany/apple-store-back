{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/extensions": "off",
    "no-magic-numbers": "error",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off"
  }
}
